FILENAME = do-op
SRC = ft_putstr.c ft_putchar.c ft_putnbr.c main.c ft_atoi.c ft_strlen.c

all: 
	@gcc -o $(FILENAME) -Wall -Werror -Wextra $(SRC)

clean:
	@rm -f $(FILENAME)

